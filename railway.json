{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "DOCKERFILE"
    },
    "cron": {
        "update_matches": "*/4 * * * * cd /app && python manage.py update_matches",
        "update_fixtures": "*/6 * * * * cd /app && python manage.py update_fixtures"
    }
}